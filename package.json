{
  "name": "fe-quality-training-project",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint:format": "pnpm lint:fix && pnpm format",
    "test": "vitest",
    "e2e": "playwright test",
    "lhci:collect": "lhci collect --url=http://localhost:3000 --numberOfRuns=1 || true",
    "lhci:assert": "lhci assert || true"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3",
    "chart.js": "4.4.3",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "prettier": "^3.6.2",
    "typescript": "^5",
    "@lhci/cli": "0.13.0",
    "@playwright/test": "1.48.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "vitest": "3.0.5"
  }
}
